<script type="text/javascript" src="{{'APP_URL' | env}}assets/js/vue_main.js?v={{'APP_VERSION' | env}}"></script>
{% endif %}
{% if isAdmin() %}
<script type="text/javascript" src="{{'APP_URL' | env}}assets/js/vue_main.js?v={{'APP_VERSION' | env}}"></script>
{% endif %}
<script type="text/javascript" src="{{'APP_URL' | env}}assets/js/vendor.js?v={{'APP_VERSION' | env}}"></script>
<script type="text/javascript" src="{{'APP_URL' | env}}assets/js/manifest.js?v={{'APP_VERSION' | env}}"></script>

</html>